{
  "name": "srcomp-screens",
  "version": "2.0.0",
  "description": "SRComp Screens",
  "author": "Student Robotics",
  "license": "MIT",
  "main": "src/index.js",
  "private": true,
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --mode development",
    "build": "cross-env NODE_ENV=production webpack --mode production --progress",
    "start": "node ./express-server.js",
    "lint": "eslint src/",
    "lint:lockfile": "lockfile-lint --path package-lock.json --type npm --validate-https --allowed-hosts registry.npmjs.org",
    "lint:spelling": "cspell --no-progress --unique",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,scss}": [
      "prettier --write --tab-width 2"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.9.1",
    "core-js": "3.27.2",
    "luxon": "3.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.5",
    "react-router-dom": "6.7.0",
    "whatwg-fetch": "3.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/eslint-parser": "7.19.1",
    "@babel/plugin-transform-react-jsx": "7.20.13",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "babel-loader": "9.1.2",
    "buffer": "6.0.3",
    "cross-env": "7.0.3",
    "cspell": "6.19.2",
    "css-loader": "6.7.3",
    "dotenv": "16.0.3",
    "eslint": "8.32.0",
    "eslint-import-resolver-webpack": "0.13.2",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-react": "7.32.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "lockfile-lint": "4.10.0",
    "mini-css-extract-plugin": "2.7.2",
    "path-browserify": "1.0.1",
    "prettier": "2.8.3",
    "sass": "1.57.1",
    "sass-loader": "13.2.0",
    "style-loader": "3.3.1",
    "webpack": "5.82.1",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.11.1"
  }
}
